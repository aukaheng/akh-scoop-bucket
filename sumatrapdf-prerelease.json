{
  "version": "16939",
  "description": "Free PDF, eBook reader.",
  "homepage": "https://www.sumatrapdfreader.org/prerelease.html",
  "architecture": {
    "64bit": {
      "url": "https://files2.sumatrapdfreader.org/software/sumatrapdf/prerel/16942/SumatraPDF-prerel-64.exe",
