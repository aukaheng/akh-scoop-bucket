{
  "version": "17021",
  "description": "Free PDF, eBook reader.",
  "homepage": "https://www.sumatrapdfreader.org/prerelease.html",
  "architecture": {
    "64bit": {
      "url": "https://files2.sumatrapdfreader.org/software/sumatrapdf/prerel/17021/SumatraPDF-prerel-64.exe",
      "hash": "AD8674968F94DBBF669181BA7BEB3E028AC18FA4CFA7C2118585B4F5EACD985F"
    }
  },
  "pre_install": [
    "$file = 'SumatraPDF-settings.txt'",
    "if (-not (Test-Path \"$persist_dir\\$file\")) {",
    "    Write-Host 'File' $file 'does not exists. Creating.' -f Yellow",
    "    $CONT = @('CheckForUpdates = false')",
    "    Set-Content \"$dir\\$file\" ($CONT -join \"`r`n\") -Encoding Ascii",
    "}"
  ],
  "bin": [
    [
      "SumatraPDF-prerel-64.exe",
      "SumatraPDF"
    ]
  ],
  "shortcuts": [
    [
      "SumatraPDF-prerel-64.exe",
      "SumatraPDF"
    ]
  ],
  "persist": "SumatraPDF-settings.txt",
  "notes": ""
}
