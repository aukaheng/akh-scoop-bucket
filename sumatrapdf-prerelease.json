{
  "version": "11593",
  "description": "Free PDF, eBook reader.",
  "homepage": "https://www.sumatrapdfreader.org",
  "license": "GPL-3.0-only,BSD-2-Clause",
  "architecture": {
    "64bit": {
      "url": "https://kjkpub.s3.amazonaws.com/software/sumatrapdf/prerel/SumatraPDF-prerelease-11593-64.exe",
      "hash": "E33BAD80CB53519784AF3F36D45596E027AFDE444781CB4FC0475EBFE253EBBA"
    }
  },
  "pre_install": [
    "$file = 'SumatraPDF-settings.txt'",
    "if (-not (Test-Path \"$persist_dir\\$file\")) {",
    "    Write-Host 'File' $file 'does not exists. Creating.' -f Yellow",
    "    $CONT = @('CheckForUpdates = false')",
    "    Set-Content \"$dir\\$file\" ($CONT -join \"`r`n\") -Encoding Ascii",
    "}"
  ],
  "bin": [
    [
      "SumatraPDF-prerelease-11593-64.exe",
      "SumatraPDF"
    ]
  ],
  "shortcuts": [
    [
      "SumatraPDF-prerelease-11593-64.exe",
      "SumatraPDF"
    ]
  ],
  "persist": "SumatraPDF-settings.txt",
  "notes": "Run the .exe with the -register-for-pdf flag. Extract libmupdf.dll and PdfPreview.dll into somewhere else and use regsvr32.exe to activate the thumbnail preview."
}
