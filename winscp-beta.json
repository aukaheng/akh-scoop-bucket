{
  "version": "6.6",
  "homepage": "https://sourceforge.net/projects/winscp/files/WinSCP/",
  "url": "https://downloads.sourceforge.net/project/winscp/WinSCP/6.6%20beta/WinSCP-6.6.beta-Portable.zip",
  "hash": "A31DF27C4AEE25B4089121DB0B339C6AC102F21E770FEC53BDE8E981F9C1D77B",
  "pre_install": [
    "if (!(Test-Path \"$persist_dir\\winscp.ini\")) {",
    "    @(",
    "        '[Configuration\\Interface]'",
    "        'RandomSeedFile=.\\winscp.rnd'",
    "    ) | Add-Content -Path \"$dir\\winscp.ini\" -Encoding ASCII -Force",
    "}"
  ],
  "bin": [
    "WinSCP.exe",
    [
      "WinSCP.com",
      "WinSCP-cli"
    ]
  ],
  "shortcuts": [
    [
      "WinSCP.exe",
      "WinSCP"
    ]
  ],
  "persist": "winscp.ini"
}
